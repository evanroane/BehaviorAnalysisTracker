@*<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <base href="/" />
    <title>Behavior Analysis Tracker</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimal-ui" />
    <meta name="apple-mobile-web-app-status-bar-style" content="yes" />
    <link rel="shortcut icon" href="/baticon.png" type="image/x-icon" />
    <link rel="stylesheet" href="~/mobile-angular-ui/css/mobile-angular-ui-hover.css" />
    <link rel="stylesheet" href="~/mobile-angular-ui/css/mobile-angular-ui-base.css" />
    <link rel="stylesheet" href="~/mobile-angular-ui/css/mobile-angular-ui-desktop.css" />
    <link rel="stylesheet" href="~/Content/custom.css" />
    <link rel="stylesheet" href="~/Content/session.css" />
    @Scripts.Render("~/bundles/modernizr")
</head>
<body ng-app="batApp" ng-controller="MainController">
    <div  ui-track-as-search-param='true' class="sidebar sidebar-left">
        <div class="scrollable">
            <h1 class="scrollable-header app-name">
                <img src="~/img/bat-logo.svg" class="sm-batlogo" alt="BAT Logo" />
                Bat <small>1.0.1</small>
            </h1>
            <div class="scrollable-content">
                <div class="list-group" ui-turn-off='uiSidebarLeft'>
                    <a class="list-group-item" href="#/">
                        <i class="fa fa-home fa-lg"></i> Home <i class="fa fa-chevron-right pull-right"></i>
                    </a>
                    <a ng-hide="user" class="list-group-item" href="#/login">
                        <i class="fa fa-unlock fa-lg"></i> Log In <i class="fa fa-chevron-right pull-right"></i>
                    </a>
                    <a ng-hide="user" class="list-group-item" href="#/register">
                        <i class="fa fa-key fa-lg"></i> Create Account <i class="fa fa-chevron-right pull-right"></i>
                    </a>
                    <a ng-show="user" class="list-group-item" href="#/mydashboard">
                        <i class="fa fa-tachometer fa-lg"></i>
                        My Dashboard <i class="fa fa-chevron-right pull-right"></i>
                    </a>
                    <a ng-show="user" class="list-group-item" href="#/changepassword">
                        <i class="fa fa-key"></i>
                        Change Password <i class="fa fa-chevron-right pull-right"></i>
                    </a>
                    <a ng-show="user" class="list-group-item" href="#/logout">
                        <i class="fa fa-lock fa-lg"></i> Log Out <i class="fa fa-chevron-right pull-right"></i>
                    </a>
                </div>
            </div>
        </div>
      </div>
    
    <div class="app">

        <div class="navbar navbar-app navbar-absolute-top">
            <div class="navbar-brand navbar-brand-center" ui-yield-to="title">
                B.A.T.
            </div>
            <div class="btn-group pull-left">
                <div ui-toggle="uiSidebarLeft" class="btn sidebar-toggle">
                    <i class="fa fa-list"></i> Menu
                </div>
            </div>
        </div>
        <div class="app-body" ng-class="{loading: loading}">
            <div ng-show="loading" class="app-content-loading">
                <i class="fa fa-circle-o-notch fa-spin loading-spinner"></i>
            </div>
            <div class="app-content">
                @RenderBody()
            </div>
        </div>

    </div>
    @Html.Partial("_LoginPartial")


    <div ui-yield-to="modals"></div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/js/angular.min.js"></script>
    <script src="~/Scripts/js/angular-route.min.js"></script>
    <script src="~/Scripts/js/mobile-angular-ui.min.js"></script>
    <script src="~/Scripts/js/jquery.min.js"></script>
    <script src="~/Scripts/js/underscore-min.js"></script>
    <script src="~/Scripts/js/moment.js"></script>
    <script src="~/Scripts/App/main.js"></script>
    <script src="~/Scripts/App/main.config.js"></script>
    <script src="~/Scripts/App/main.constants.js"></script>
    <script src="~/Scripts/App/main.controller.js"></script>
    <script src="~/Scripts/App/dashboard/dashboard.routes.js"></script>
    <script src="~/Scripts/App/dashboard/factories/behaviorevent.factory.js"></script>
    <script src="~/Scripts/App/dashboard/factories/codesets.factory.js"></script>
    <script src="~/Scripts/App/dashboard/factories/input.factory.js"></script>
    <script src="~/Scripts/App/dashboard/factories/session.factory.js"></script>
    <script src="~/Scripts/App/dashboard/controllers/codeset.controller.js"></script>
    <script src="~/Scripts/App/dashboard/controllers/input.controller.js"></script>
    <script src="~/Scripts/App/dashboard/controllers/session.controller.js"></script>
    <script src="~/Scripts/App/dashboard/controllers/behaviorevent.controller.js"></script>
    <script src="~/Scripts/App/dashboard/controllers/download.controller.js"></script>
</body>
</html>*@